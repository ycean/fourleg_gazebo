<?xml version="1.0" encoding="UTF-8"?>
<launch>

  <include file="$(find fourleg_gazebo)/launch/spawn_robot_urdf.launch">
    <arg name="x" default="0"/>
    <arg name="y" default="0"/>
    <arg name="z" default="0.09"/>
    <arg name="roll" default="0"/>
    <arg name="pitch" default="0"/>
    <arg name="yaw" default="0"/>
    <arg name="urdf_robot_file" default="$(find fourleg_gazebo)/urdf/robot_control.urdf"/>
    <arg name="robot_name" default="fourleg"/>
  </include>
</launch>
